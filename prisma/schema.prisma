generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Reflection {
  id           String @id @default(cuid())
  date         String @unique
  items        Json
  selectedItem Json?
  chatMessages Json
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
